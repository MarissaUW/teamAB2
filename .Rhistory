getColor <- function(set){
sapply(set$n, function(n){
if (n <= 1000){
"blue"
} else if (n <= 5000){
"springgreen"
} else if (n <= 10000){
"orange"
} else {
"red"
}
})
}
group_name <- function(set){
sapply(set$n, function(n){
if (n <= 1000){
"Crimes <= 1000"
} else if (n <= 5000){
"1000 < Crimes <= 5000"
} else if (n <= 10000){
"5000 < Crimes <= 10000"
} else {
"10000 < Crimes <= 23024"
}
})
}
