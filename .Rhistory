color = getColor(neighborhoods),
group = group_name(neighborhoods)) %>%
addLayersControl(overlayGroups = c("Crimes <= 1000", "1000 < Crimes <= 5000", "5000 < Crimes <= 10000", "10000 < Crimes <= 23024")) %>%
setView(lng=-122.3321, lat=47.6062, zoom = 11)
